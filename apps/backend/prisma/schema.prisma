datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model LeadBruto {
  id             String   @id @default(uuid())
  classe         String
  subclasse      String?
  grupoTensao    String?
  modalidade     String?
  distribuidora  String
  municipio      String
  estado         String
  latitude       Float?
  longitude      Float?
  dataConexao    DateTime?
  dataImportacao DateTime @default(now())
}

model Distribuidora {
  id         String   @id @default(uuid())
  nome       String
  regiao     String?
  uf         String
  cnpj       String?
  outorga    String?
  tipo       String?
  grupo      String?
  subgrupo   String?
  classe     String?
}
